class Solution {
public:
    bool isValid(string s) {
         stack<char> st;
        for(int i = 0;i<s.size();i++){
            if(s[i]=='(' || s[i]=='[' || s[i]=='{'){                 
                if(s[i]=='(') st.push(')');
                if(s[i]=='[') st.push(']');
                if(s[i]=='{') st.push('}');
            }
            else{
                if(!st.empty() && s[i]==st.top()) st.pop();          
				else return false;                                  
            }
        }
        return st.empty();
    }
};
